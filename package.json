{
  "name": "cardforge-ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:backend": "wrangler pages dev . --d1 DB=cardforge-db --r2 BUCKET=cardforge-assets --binding PUBLIC_R2_URL=http://localhost:8787/r2",
    "build": "npm run build:functions && vite build",
    "build:functions": "esbuild functions/api/generate.ts --bundle --outfile=functions/api/generate.js --platform=node --format=esm --target=esnext",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240208.0",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "esbuild": "^0.20.2",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "wrangler": "^3.0.0"
  }
}
